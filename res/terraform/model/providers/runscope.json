{
  "name": "runscope",
  "type": "provider",
  "version": "v0.5.0-2-gf380a04",
  "provider": {
    "access_token": {
      "Type": "String",
      "Required": true,
      "Description": "A runscope access token."
    },
    "api_url": {
      "Type": "String",
      "Optional": true,
      "Description": "A runscope api url i.e. https://api.runscope.com."
    }
  },
  "resources": {
    "runscope_bucket": {
      "name": {
        "Type": "String",
        "Required": true
      },
      "team_uuid": {
        "Type": "String",
        "Required": true
      }
    },
    "runscope_environment": {
      "bucket_id": {
        "Type": "String",
        "Required": true
      },
      "emails": {
        "Type": "List",
        "Optional": true,
        "MaxItems": 1,
        "Elem": {
          "Type": "SchemaInfo",
          "Info": {
            "notify_all": {
              "Type": "Bool",
              "Required": true
            },
            "notify_on": {
              "Type": "String",
              "Required": true
            },
            "notify_threshold": {
              "Type": "Int",
              "Required": true
            },
            "recipients": {
              "Type": "Set",
              "Required": true,
              "Elem": {
                "Type": "SchemaInfo",
                "Info": {
                  "email": {
                    "Type": "String",
                    "Optional": true
                  },
                  "id": {
                    "Type": "String",
                    "Optional": true
                  },
                  "name": {
                    "Type": "String",
                    "Optional": true
                  }
                }
              }
            }
          }
        }
      },
      "initial_variables": {
        "Type": "Map",
        "Optional": true,
        "Elem": {
          "Type": "SchemaElements",
          "ElementsType": "String"
        }
      },
      "integrations": {
        "Type": "Set",
        "Optional": true,
        "Elem": {
          "Type": "SchemaElements",
          "ElementsType": "String"
        }
      },
      "name": {
        "Type": "String",
        "Required": true
      },
      "preserve_cookies": {
        "Type": "Bool",
        "Optional": true
      },
      "regions": {
        "Type": "Set",
        "Optional": true,
        "Elem": {
          "Type": "SchemaElements",
          "ElementsType": "String"
        }
      },
      "remote_agents": {
        "Type": "Set",
        "Optional": true,
        "Elem": {
          "Type": "SchemaInfo",
          "Info": {
            "name": {
              "Type": "String",
              "Required": true
            },
            "uuid": {
              "Type": "String",
              "Required": true
            }
          }
        }
      },
      "retry_on_failure": {
        "Type": "Bool",
        "Optional": true
      },
      "script": {
        "Type": "String",
        "Optional": true
      },
      "test_id": {
        "Type": "String",
        "Optional": true
      },
      "verify_ssl": {
        "Type": "Bool",
        "Optional": true
      },
      "webhooks": {
        "Type": "Set",
        "Optional": true,
        "Elem": {
          "Type": "SchemaElements",
          "ElementsType": "String"
        }
      }
    },
    "runscope_schedule": {
      "bucket_id": {
        "Type": "String",
        "Required": true
      },
      "environment_id": {
        "Type": "String",
        "Required": true
      },
      "interval": {
        "Type": "String",
        "Optional": true
      },
      "note": {
        "Type": "String",
        "Optional": true
      },
      "test_id": {
        "Type": "String",
        "Required": true
      }
    },
    "runscope_step": {
      "assertions": {
        "Type": "List",
        "Optional": true,
        "Elem": {
          "Type": "SchemaInfo",
          "Info": {
            "comparison": {
              "Type": "String",
              "Required": true
            },
            "property": {
              "Type": "String",
              "Optional": true
            },
            "source": {
              "Type": "String",
              "Required": true
            },
            "value": {
              "Type": "String",
              "Optional": true
            }
          }
        }
      },
      "auth": {
        "Type": "Set",
        "Optional": true,
        "Elem": {
          "Type": "SchemaInfo",
          "Info": {
            "auth_type": {
              "Type": "String",
              "Required": true
            },
            "password": {
              "Type": "String",
              "Required": true
            },
            "username": {
              "Type": "String",
              "Required": true
            }
          }
        }
      },
      "before_scripts": {
        "Type": "List",
        "Optional": true,
        "Elem": {
          "Type": "SchemaElements",
          "ElementsType": "String"
        }
      },
      "body": {
        "Type": "String",
        "Optional": true
      },
      "bucket_id": {
        "Type": "String",
        "Required": true
      },
      "headers": {
        "Type": "Set",
        "Optional": true,
        "Elem": {
          "Type": "SchemaInfo",
          "Info": {
            "header": {
              "Type": "String",
              "Required": true
            },
            "value": {
              "Type": "String",
              "Required": true
            }
          }
        }
      },
      "method": {
        "Type": "String",
        "Optional": true
      },
      "note": {
        "Type": "String",
        "Optional": true
      },
      "scripts": {
        "Type": "List",
        "Optional": true,
        "Elem": {
          "Type": "SchemaElements",
          "ElementsType": "String"
        }
      },
      "step_type": {
        "Type": "String",
        "Required": true
      },
      "test_id": {
        "Type": "String",
        "Required": true
      },
      "url": {
        "Type": "String",
        "Optional": true
      },
      "variables": {
        "Type": "Set",
        "Optional": true,
        "Elem": {
          "Type": "SchemaInfo",
          "Info": {
            "name": {
              "Type": "String",
              "Required": true
            },
            "property": {
              "Type": "String",
              "Optional": true
            },
            "source": {
              "Type": "String",
              "Required": true
            }
          }
        }
      }
    },
    "runscope_test": {
      "bucket_id": {
        "Type": "String",
        "Required": true
      },
      "default_environment_id": {
        "Type": "String",
        "Computed": true
      },
      "description": {
        "Type": "String",
        "Required": true
      },
      "name": {
        "Type": "String",
        "Required": true
      }
    }
  },
  "data-sources": {
    "runscope_bucket": {
      "key": {
        "Type": "String",
        "Required": true
      },
      "name": {
        "Type": "String",
        "Computed": true
      },
      "team_uuid": {
        "Type": "String",
        "Computed": true
      }
    },
    "runscope_buckets": {
      "filter": {
        "Type": "Set",
        "Optional": true,
        "Elem": {
          "Type": "SchemaInfo",
          "Info": {
            "name": {
              "Type": "String",
              "Required": true
            },
            "values": {
              "Type": "Set",
              "Required": true,
              "Elem": {
                "Type": "SchemaElements",
                "ElementsType": "String"
              }
            }
          }
        }
      },
      "keys": {
        "Type": "Set",
        "Computed": true,
        "Elem": {
          "Type": "SchemaElements",
          "ElementsType": "String"
        }
      }
    },
    "runscope_integration": {
      "description": {
        "Type": "String",
        "Computed": true
      },
      "filter": {
        "Type": "Set",
        "Optional": true,
        "Elem": {
          "Type": "SchemaInfo",
          "Info": {
            "name": {
              "Type": "String",
              "Required": true
            },
            "values": {
              "Type": "Set",
              "Required": true,
              "Elem": {
                "Type": "SchemaElements",
                "ElementsType": "String"
              }
            }
          }
        }
      },
      "team_uuid": {
        "Type": "String",
        "Required": true
      },
      "type": {
        "Type": "String",
        "Required": true
      }
    },
    "runscope_integrations": {
      "filter": {
        "Type": "Set",
        "Optional": true,
        "Elem": {
          "Type": "SchemaInfo",
          "Info": {
            "name": {
              "Type": "String",
              "Required": true
            },
            "values": {
              "Type": "Set",
              "Required": true,
              "Elem": {
                "Type": "SchemaElements",
                "ElementsType": "String"
              }
            }
          }
        }
      },
      "ids": {
        "Type": "Set",
        "Computed": true,
        "Elem": {
          "Type": "SchemaElements",
          "ElementsType": "String"
        }
      },
      "team_uuid": {
        "Type": "String",
        "Required": true
      }
    }
  }
}