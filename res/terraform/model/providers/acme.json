{
  ".schema_version": "1",
  "name": "acme",
  "type": "provider",
  "version": "v1.1.1",
  "provider": {
    "server_url": {
      "Type": "String",
      "Required": true,
      "DefaultFunc": "ENV(ACME_SERVER_URL)"
    }
  },
  "resources": {
    "acme_certificate": {
      "account_key_pem": {
        "Type": "String",
        "Required": true
      },
      "certificate_domain": {
        "Type": "String",
        "Computed": true
      },
      "certificate_p12": {
        "Type": "String",
        "Computed": true
      },
      "certificate_p12_password": {
        "Type": "String",
        "Optional": true,
        "Default": {
          "Type": "string"
        }
      },
      "certificate_pem": {
        "Type": "String",
        "Computed": true
      },
      "certificate_request_pem": {
        "Type": "String",
        "Optional": true,
        "ConflictsWith": [
          "common_name",
          "subject_alternative_names",
          "key_type"
        ]
      },
      "certificate_url": {
        "Type": "String",
        "Computed": true
      },
      "common_name": {
        "Type": "String",
        "Optional": true,
        "ConflictsWith": [
          "certificate_request_pem"
        ]
      },
      "dns_challenge": {
        "Type": "List",
        "Required": true,
        "MaxItems": 1,
        "Elem": {
          "Type": "SchemaInfo",
          "Info": {
            "config": {
              "Type": "Map",
              "Optional": true
            },
            "provider": {
              "Type": "String",
              "Required": true
            },
            "recursive_nameservers": {
              "Type": "List",
              "Optional": true,
              "Elem": {
                "Type": "SchemaElements",
                "ElementsType": "String"
              }
            }
          }
        }
      },
      "issuer_pem": {
        "Type": "String",
        "Computed": true
      },
      "key_type": {
        "Type": "String",
        "Optional": true,
        "ConflictsWith": [
          "certificate_request_pem"
        ],
        "Default": {
          "Type": "string",
          "Value": "2048"
        }
      },
      "min_days_remaining": {
        "Type": "Int",
        "Optional": true,
        "Default": {
          "Type": "int",
          "Value": "7"
        }
      },
      "must_staple": {
        "Type": "Bool",
        "Optional": true,
        "Default": {
          "Type": "bool",
          "Value": "false"
        }
      },
      "private_key_pem": {
        "Type": "String",
        "Computed": true
      },
      "subject_alternative_names": {
        "Type": "Set",
        "Optional": true,
        "ConflictsWith": [
          "certificate_request_pem"
        ],
        "Elem": {
          "Type": "SchemaElements",
          "ElementsType": "String"
        }
      }
    },
    "acme_registration": {
      "account_key_pem": {
        "Type": "String",
        "Required": true
      },
      "email_address": {
        "Type": "String",
        "Required": true
      },
      "registration_url": {
        "Type": "String",
        "Computed": true
      }
    }
  },
  "data-sources": {}
}