{
  ".schema_version": "1",
  "name": "softlayer",
  "type": "provider",
  "version": "v0.0.1",
  "provider": {
    "api_key": {
      "Type": "String",
      "Required": true,
      "Description": "The API key for SoftLayer API operations.",
      "DefaultFunc": "ENV(SOFTLAYER_API_KEY)"
    },
    "username": {
      "Type": "String",
      "Required": true,
      "Description": "The user name for SoftLayer API operations.",
      "DefaultFunc": "ENV(SOFTLAYER_USERNAME)"
    }
  },
  "resources": {
    "softlayer_ssh_key": {
      "fingerprint": {
        "Type": "String",
        "Computed": true
      },
      "id": {
        "Type": "Int",
        "Computed": true
      },
      "name": {
        "Type": "String",
        "Required": true
      },
      "notes": {
        "Type": "String",
        "Optional": true
      },
      "public_key": {
        "Type": "String",
        "Required": true
      }
    },
    "softlayer_virtual_guest": {
      "backend_vlan_id": {
        "Type": "String",
        "Optional": true
      },
      "block_device_template_group_gid": {
        "Type": "String",
        "Optional": true
      },
      "cpu": {
        "Type": "Int",
        "Required": true
      },
      "dedicated_acct_host_only": {
        "Type": "Bool",
        "Optional": true
      },
      "disks": {
        "Type": "List",
        "Optional": true,
        "Elem": {
          "Type": "SchemaElements",
          "ElementsType": "Int"
        }
      },
      "domain": {
        "Type": "String",
        "Required": true
      },
      "frontend_vlan_id": {
        "Type": "String",
        "Optional": true
      },
      "hourly_billing": {
        "Type": "Bool",
        "Required": true
      },
      "image": {
        "Type": "String",
        "Optional": true
      },
      "ipv4_address": {
        "Type": "String",
        "Computed": true
      },
      "ipv4_address_private": {
        "Type": "String",
        "Computed": true
      },
      "local_disk": {
        "Type": "Bool",
        "Required": true
      },
      "name": {
        "Type": "String",
        "Required": true
      },
      "post_install_script_uri": {
        "Type": "String",
        "Optional": true
      },
      "private_network_only": {
        "Type": "Bool",
        "Optional": true,
        "Default": {
          "Type": "bool",
          "Value": "false"
        }
      },
      "public_network_speed": {
        "Type": "Int",
        "Optional": true,
        "Default": {
          "Type": "int",
          "Value": "1000"
        }
      },
      "ram": {
        "Type": "Int",
        "Required": true
      },
      "region": {
        "Type": "String",
        "Required": true
      },
      "ssh_keys": {
        "Type": "List",
        "Optional": true,
        "Elem": {
          "Type": "SchemaElements",
          "ElementsType": "Int"
        }
      },
      "user_data": {
        "Type": "String",
        "Optional": true
      }
    }
  },
  "data-sources": {}
}