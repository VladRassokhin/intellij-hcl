{
  ".schema_version": "1",
  "name": "dme",
  "type": "provider",
  "version": "v0.1.0",
  "provider": {
    "akey": {
      "Type": "String",
      "Required": true,
      "Description": "A DNSMadeEasy API Key.",
      "DefaultFunc": "UNKNOWN"
    },
    "skey": {
      "Type": "String",
      "Required": true,
      "Description": "The Secret Key for API operations.",
      "DefaultFunc": "UNKNOWN"
    },
    "usesandbox": {
      "Type": "Bool",
      "Required": true,
      "Description": "If true, use the DME Sandbox.",
      "DefaultFunc": "UNKNOWN"
    }
  },
  "resources": {
    "dme_record": {
      "description": {
        "Type": "String",
        "Optional": true
      },
      "domainid": {
        "Type": "String",
        "Required": true
      },
      "gtdLocation": {
        "Type": "String",
        "Optional": true
      },
      "hardLink": {
        "Type": "Bool",
        "Optional": true
      },
      "keywords": {
        "Type": "String",
        "Optional": true
      },
      "mxLevel": {
        "Type": "Int",
        "Optional": true
      },
      "name": {
        "Type": "String",
        "Required": true
      },
      "port": {
        "Type": "Int",
        "Optional": true
      },
      "priority": {
        "Type": "Int",
        "Optional": true
      },
      "redirectType": {
        "Type": "String",
        "Optional": true
      },
      "title": {
        "Type": "String",
        "Optional": true
      },
      "ttl": {
        "Type": "Int",
        "Optional": true
      },
      "type": {
        "Type": "String",
        "Required": true
      },
      "value": {
        "Type": "String",
        "Required": true
      },
      "weight": {
        "Type": "Int",
        "Optional": true
      }
    }
  },
  "data-sources": {}
}