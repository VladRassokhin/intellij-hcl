{
  ".schema_version": "1",
  "name": "statuscake",
  "type": "provider",
  "version": "v0.2.0",
  "provider": {
    "apikey": {
      "Type": "String",
      "Required": true,
      "Description": "API Key for StatusCake",
      "DefaultFunc": "ENV(STATUSCAKE_APIKEY)"
    },
    "username": {
      "Type": "String",
      "Required": true,
      "Description": "Username for StatusCake Account.",
      "DefaultFunc": "ENV(STATUSCAKE_USERNAME)"
    }
  },
  "resources": {
    "statuscake_test": {
      "basic_pass": {
        "Type": "String",
        "Optional": true
      },
      "basic_user": {
        "Type": "String",
        "Optional": true
      },
      "branding": {
        "Type": "Int",
        "Optional": true
      },
      "check_rate": {
        "Type": "Int",
        "Optional": true,
        "Default": {
          "Type": "int",
          "Value": "300"
        }
      },
      "confirmations": {
        "Type": "Int",
        "Optional": true
      },
      "contact_id": {
        "Type": "Int",
        "Optional": true
      },
      "custom_header": {
        "Type": "String",
        "Optional": true
      },
      "do_not_find": {
        "Type": "Bool",
        "Optional": true
      },
      "final_endpoint": {
        "Type": "String",
        "Optional": true
      },
      "find_string": {
        "Type": "String",
        "Optional": true
      },
      "follow_redirect": {
        "Type": "Bool",
        "Optional": true
      },
      "logo_image": {
        "Type": "String",
        "Optional": true
      },
      "node_locations": {
        "Type": "Set",
        "Optional": true,
        "Elem": {
          "Type": "SchemaElements",
          "ElementsType": "String"
        }
      },
      "paused": {
        "Type": "Bool",
        "Optional": true,
        "Default": {
          "Type": "bool",
          "Value": "false"
        }
      },
      "ping_url": {
        "Type": "String",
        "Optional": true
      },
      "port": {
        "Type": "Int",
        "Optional": true
      },
      "post_raw": {
        "Type": "String",
        "Optional": true
      },
      "public": {
        "Type": "Int",
        "Optional": true
      },
      "real_browser": {
        "Type": "Int",
        "Optional": true
      },
      "status": {
        "Type": "String",
        "Computed": true
      },
      "status_codes": {
        "Type": "String",
        "Optional": true
      },
      "test_id": {
        "Type": "String",
        "Computed": true
      },
      "test_tags": {
        "Type": "String",
        "Optional": true
      },
      "test_type": {
        "Type": "String",
        "Required": true
      },
      "timeout": {
        "Type": "Int",
        "Optional": true,
        "Default": {
          "Type": "int",
          "Value": "40"
        }
      },
      "trigger_rate": {
        "Type": "Int",
        "Optional": true,
        "Default": {
          "Type": "int",
          "Value": "5"
        }
      },
      "uptime": {
        "Type": "Float",
        "Computed": true
      },
      "use_jar": {
        "Type": "Int",
        "Optional": true
      },
      "user_agent": {
        "Type": "String",
        "Optional": true
      },
      "virus": {
        "Type": "Int",
        "Optional": true
      },
      "website_host": {
        "Type": "String",
        "Optional": true
      },
      "website_name": {
        "Type": "String",
        "Required": true
      },
      "website_url": {
        "Type": "String",
        "Required": true
      }
    }
  },
  "data-sources": {}
}