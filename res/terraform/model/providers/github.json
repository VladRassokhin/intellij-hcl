{
  ".schema_version": "1",
  "name": "github",
  "type": "provider",
  "version": "v1.3.0",
  "provider": {
    "base_url": {
      "Type": "String",
      "Optional": true,
      "Description": "The GitHub Base API URL",
      "DefaultFunc": "ENV(GITHUB_BASE_URL)"
    },
    "insecure": {
      "Type": "Bool",
      "Optional": true,
      "Description": "Whether server should be accessed without verifying the TLS certificate.",
      "Default": {
        "Type": "bool",
        "Value": "false"
      }
    },
    "organization": {
      "Type": "String",
      "Required": true,
      "Description": "The GitHub organization name to manage.",
      "DefaultFunc": "ENV(GITHUB_ORGANIZATION)"
    },
    "token": {
      "Type": "String",
      "Required": true,
      "Description": "The OAuth token used to connect to GitHub.",
      "DefaultFunc": "ENV(GITHUB_TOKEN)"
    }
  },
  "resources": {
    "github_branch_protection": {
      "branch": {
        "Type": "String",
        "Required": true
      },
      "enforce_admins": {
        "Type": "Bool",
        "Optional": true,
        "Default": {
          "Type": "bool",
          "Value": "false"
        }
      },
      "etag": {
        "Type": "String",
        "Computed": true
      },
      "repository": {
        "Type": "String",
        "Required": true
      },
      "required_pull_request_reviews": {
        "Type": "List",
        "Optional": true,
        "MaxItems": 1,
        "Elem": {
          "Type": "SchemaInfo",
          "Info": {
            "dismiss_stale_reviews": {
              "Type": "Bool",
              "Optional": true,
              "Default": {
                "Type": "bool",
                "Value": "false"
              }
            },
            "dismissal_teams": {
              "Type": "Set",
              "Optional": true,
              "Elem": {
                "Type": "SchemaElements",
                "ElementsType": "String"
              }
            },
            "dismissal_users": {
              "Type": "Set",
              "Optional": true,
              "Elem": {
                "Type": "SchemaElements",
                "ElementsType": "String"
              }
            },
            "include_admins": {
              "Type": "Bool",
              "Optional": true,
              "Deprecated": "Use enforce_admins instead",
              "Default": {
                "Type": "bool",
                "Value": "false"
              }
            },
            "require_code_owner_reviews": {
              "Type": "Bool",
              "Optional": true
            }
          }
        }
      },
      "required_status_checks": {
        "Type": "List",
        "Optional": true,
        "MaxItems": 1,
        "Elem": {
          "Type": "SchemaInfo",
          "Info": {
            "contexts": {
              "Type": "Set",
              "Optional": true,
              "Elem": {
                "Type": "SchemaElements",
                "ElementsType": "String"
              }
            },
            "include_admins": {
              "Type": "Bool",
              "Optional": true,
              "Deprecated": "Use enforce_admins instead",
              "Default": {
                "Type": "bool",
                "Value": "false"
              }
            },
            "strict": {
              "Type": "Bool",
              "Optional": true,
              "Default": {
                "Type": "bool",
                "Value": "false"
              }
            }
          }
        }
      },
      "restrictions": {
        "Type": "List",
        "Optional": true,
        "MaxItems": 1,
        "Elem": {
          "Type": "SchemaInfo",
          "Info": {
            "teams": {
              "Type": "Set",
              "Optional": true,
              "Elem": {
                "Type": "SchemaElements",
                "ElementsType": "String"
              }
            },
            "users": {
              "Type": "Set",
              "Optional": true,
              "Elem": {
                "Type": "SchemaElements",
                "ElementsType": "String"
              }
            }
          }
        }
      }
    },
    "github_issue_label": {
      "color": {
        "Type": "String",
