      },
      "job_id": {
        "Type": "String",
        "Required": true,
        "Description": "Job ID"
      },
      "job_modify_index": {
        "Type": "Int",
        "Description": "Job Modify Index",
        "Computed": true
      },
      "modify_index": {
        "Type": "Int",
        "Description": "Modify Index",
        "Computed": true
      },
      "name": {
