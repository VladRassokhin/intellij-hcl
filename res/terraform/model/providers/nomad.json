        "Computed": true,
        "Elem": {
          "Type": "SchemaInfo",
          "Info": {
            "ltarget": {
              "Type": "String",
              "Computed": true
            },
            "operand": {
              "Type": "String",
              "Computed": true
            },
            "rtarget": {
              "Type": "String",
              "Computed": true
            }
          }
        }
      },
      "create_index": {
        "Type": "Int",
