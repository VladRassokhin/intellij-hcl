{
  ".schema_version": "1",
  "name": "rabbitmq",
  "type": "provider",
  "version": "v1.0.0",
  "provider": {
    "cacert_file": {
      "Type": "String",
      "Optional": true,
      "DefaultFunc": "ENV(RABBITMQ_CACERT)"
    },
    "endpoint": {
      "Type": "String",
      "Required": true,
      "DefaultFunc": "ENV(RABBITMQ_ENDPOINT)"
    },
    "insecure": {
      "Type": "Bool",
      "Optional": true,
      "DefaultFunc": "ENV(RABBITMQ_INSECURE)"
    },
    "password": {
      "Type": "String",
      "Required": true,
      "DefaultFunc": "ENV(RABBITMQ_PASSWORD)"
    },
    "username": {
      "Type": "String",
      "Required": true,
      "DefaultFunc": "ENV(RABBITMQ_USERNAME)"
    }
  },
  "resources": {
    "rabbitmq_binding": {
      "arguments": {
        "Type": "Map",
        "Optional": true
      },
      "destination": {
        "Type": "String",
        "Required": true
      },
      "destination_type": {
        "Type": "String",
        "Required": true
      },
      "properties_key": {
        "Type": "String",
        "Computed": true
      },
      "routing_key": {
        "Type": "String",
        "Optional": true
      },
      "source": {
        "Type": "String",
        "Required": true
      },
      "vhost": {
        "Type": "String",
        "Required": true
      }
    },
    "rabbitmq_exchange": {
      "name": {
        "Type": "String",
        "Required": true
      },
      "settings": {
        "Type": "List",
        "Required": true,
        "MaxItems": 1,
        "Elem": {
          "Type": "SchemaInfo",
          "Info": {
            "arguments": {
              "Type": "Map",
              "Optional": true
            },
            "auto_delete": {
              "Type": "Bool",
              "Optional": true,
              "Default": {
                "Type": "bool",
                "Value": "false"
              }
            },
            "durable": {
              "Type": "Bool",
              "Optional": true,
              "Default": {
                "Type": "bool",
                "Value": "false"
              }
            },
            "type": {
              "Type": "String",
              "Required": true
            }
          }
        }
      },
      "vhost": {
        "Type": "String",
        "Optional": true,
        "Default": {
          "Type": "string",
          "Value": "/"
        }
      }
    },
    "rabbitmq_permissions": {
      "permissions": {
        "Type": "List",
        "Required": true,
        "MaxItems": 1,
        "Elem": {
          "Type": "SchemaInfo",
          "Info": {
            "configure": {
              "Type": "String",
              "Required": true
            },
            "read": {
              "Type": "String",
              "Required": true
            },
            "write": {
              "Type": "String",
              "Required": true
            }
          }
        }
      },
      "user": {
        "Type": "String",
        "Required": true
      },
      "vhost": {
        "Type": "String",
        "Optional": true,
        "Default": {
          "Type": "string",
          "Value": "/"
        }
      }
    },
    "rabbitmq_policy": {
      "name": {
        "Type": "String",
        "Required": true
      },
      "policy": {
        "Type": "List",
        "Required": true,
        "MaxItems": 1,
        "Elem": {
          "Type": "SchemaInfo",
          "Info": {
            "apply_to": {
              "Type": "String",
              "Required": true
            },
            "definition": {
              "Type": "Map",
              "Required": true
            },
            "pattern": {
              "Type": "String",
              "Required": true
            },
            "priority": {
              "Type": "Int",
              "Required": true
            }
          }
        }
      },
      "vhost": {
        "Type": "String",
        "Required": true
      }
    },
    "rabbitmq_queue": {
      "name": {
        "Type": "String",
        "Required": true
      },
      "settings": {
        "Type": "List",
        "Required": true,
        "MaxItems": 1,
        "Elem": {
          "Type": "SchemaInfo",
          "Info": {
            "arguments": {
              "Type": "Map",
              "Optional": true,
              "ConflictsWith": [
                "settings.0.arguments_json"
              ]
            },
            "arguments_json": {
              "Type": "String",
              "Optional": true,
              "ConflictsWith": [
                "settings.0.arguments"
              ]
            },
            "auto_delete": {
              "Type": "Bool",
              "Optional": true,
              "Default": {
                "Type": "bool",
                "Value": "false"
              }
            },
            "durable": {
              "Type": "Bool",
              "Optional": true,
              "Default": {
                "Type": "bool",
                "Value": "false"
              }
            }
          }
        }
      },
      "vhost": {
        "Type": "String",
        "Optional": true,
        "Default": {
          "Type": "string",
          "Value": "/"
        }
      }
    },
    "rabbitmq_user": {
      "name": {
        "Type": "String",
        "Required": true
      },
      "password": {
        "Type": "String",
        "Required": true
      },
      "tags": {
        "Type": "List",
        "Optional": true,
        "Elem": {
          "Type": "SchemaElements",
          "ElementsType": "String"
        }
      }
    },
    "rabbitmq_vhost": {
      "name": {
        "Type": "String",
        "Required": true
      }
    }
  },
  "data-sources": {}
}