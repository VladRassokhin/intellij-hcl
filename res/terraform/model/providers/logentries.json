{
  ".schema_version": "1",
  "name": "logentries",
  "type": "provider",
  "version": "v1.0.0",
  "provider": {
    "account_key": {
      "Type": "String",
      "Required": true,
      "Description": "The Log Entries account key.",
      "DefaultFunc": "ENV(LOGENTRIES_ACCOUNT_KEY)"
    }
  },
  "resources": {
    "logentries_log": {
      "filename": {
        "Type": "String",
        "Optional": true
      },
      "logset_id": {
        "Type": "String",
        "Required": true
      },
      "name": {
        "Type": "String",
        "Required": true
      },
      "retention_period": {
        "Type": "String",
        "Optional": true,
        "Default": {
          "Type": "string",
          "Value": "ACCOUNT_DEFAULT"
        }
      },
      "source": {
        "Type": "String",
        "Optional": true,
        "Default": {
          "Type": "string",
          "Value": "token"
        }
      },
      "token": {
        "Type": "String",
        "Computed": true
      },
      "type": {
        "Type": "String",
        "Optional": true,
        "Default": {
          "Type": "string"
        }
      }
    },
    "logentries_logset": {
      "location": {
        "Type": "String",
        "Optional": true,
        "Default": {
          "Type": "string",
          "Value": "nonlocation"
        }
      },
      "name": {
        "Type": "String",
        "Required": true
      }
    }
  },
  "data-sources": {
    "logentries_logset": {
      "location": {
        "Type": "String",
        "Optional": true,
        "Default": {
          "Type": "string",
          "Value": "nonlocation"
        }
      },
      "name": {
        "Type": "String",
        "Required": true
      }
    }
  }
}