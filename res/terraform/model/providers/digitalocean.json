            },
            "updated_at": {
              "Type": "String",
              "Computed": true
            }
          }
        }
      },
      "size": {
        "Type": "String",
        "Required": true
      },
      "tags": {
        "Type": "Set",
        "Optional": true,
        "Elem": {
          "Type": "SchemaElements",
          "ElementsType": "String"
        }
      }
    },
    "digitalocean_loadbalancer": {
