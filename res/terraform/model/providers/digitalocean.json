              }
            },
            "path": {
              "Type": "String",
              "Optional": true
            },
            "port": {
              "Type": "Int",
              "Required": true
            },
            "protocol": {
              "Type": "String",
              "Required": true
            },
            "response_timeout_seconds": {
              "Type": "Int",
              "Optional": true,
              "Default": {
                "Type": "int",
                "Value": "5"
              }
            },
            "unhealthy_threshold": {
              "Type": "Int",
              "Optional": true,
              "Default": {
                "Type": "int",
                "Value": "3"
              }
            }
          }
        }
      },
      "ip": {
        "Type": "String",
        "Computed": true
      },
      "name": {
        "Type": "String",
        "Required": true
      },
      "redirect_http_to_https": {
        "Type": "Bool",
        "Optional": true,
        "Default": {
          "Type": "bool",
          "Value": "false"
        }
      },
      "region": {
        "Type": "String",
        "Required": true
      },
      "status": {
        "Type": "String",
        "Computed": true
      },
      "sticky_sessions": {
        "Type": "List",
        "Optional": true,
