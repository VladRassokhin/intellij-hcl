{
  ".schema_version": "1",
  "name": "digitalocean",
  "type": "provider",
  "version": "v1.2.0",
  "provider": {
    "api_endpoint": {
      "Type": "String",
      "Required": true,
      "Description": "The URL to use for the DigitalOcean API.",
      "DefaultFunc": "ENV(DIGITALOCEAN_API_URL)"
    },
    "spaces_access_id": {
      "Type": "String",
      "Optional": true,
      "Description": "The access key ID for Spaces API operations.",
      "DefaultFunc": "ENV(SPACES_ACCESS_KEY_ID)"
    },
    "spaces_secret_key": {
      "Type": "String",
      "Optional": true,
      "Description": "The secret access key for Spaces API operations.",
      "DefaultFunc": "ENV(SPACES_SECRET_ACCESS_KEY)"
    },
    "token": {
      "Type": "String",
      "Required": true,
      "Description": "The token key for API operations.",
      "DefaultFunc": "ENV(DIGITALOCEAN_TOKEN)"
    }
  },
  "resources": {
    "digitalocean_cdn": {
      "certificate_id": {
        "Type": "String",
        "Optional": true,
        "Description": "ID of a DigitalOcean managed TLS certificate for use with custom domains"
      },
      "created_at": {
        "Type": "String",
        "Description": "The date and time (ISO8601) of when the CDN endpoint was created.",
        "Computed": true
      },
      "custom_domain": {
        "Type": "String",
        "Optional": true,
        "Description": "fully qualified domain name (FQDN) for custom subdomain, (requires certificate_id)"
      },
      "endpoint": {
        "Type": "String",
        "Description": "fully qualified domain name (FQDN) to serve the CDN content",
        "Computed": true
      },
      "origin": {
        "Type": "String",
        "Required": true,
        "Description": "fully qualified domain name (FQDN) for the origin server"
      },
      "ttl": {
        "Type": "Int",
        "Optional": true,
        "Description": "The amount of time the content is cached in the CDN",
        "Computed": true
      }
    },
    "digitalocean_certificate": {
      "certificate_chain": {
        "Type": "String",
        "Optional": true
      },
      "domains": {
        "Type": "Set",
        "Optional": true,
        "ConflictsWith": [
          "private_key",
          "leaf_certificate",
          "certificate_chain"
        ],
        "Elem": {
          "Type": "SchemaElements",
          "ElementsType": "String"
        }
      },
      "leaf_certificate": {
        "Type": "String",
        "Optional": true
      },
      "name": {
        "Type": "String",
        "Required": true
      },
      "not_after": {
        "Type": "String",
        "Computed": true
      },
      "private_key": {
        "Type": "String",
        "Optional": true
      },
      "sha1_fingerprint": {
        "Type": "String",
        "Computed": true
      },
      "state": {
        "Type": "String",
        "Computed": true
      },
      "type": {
        "Type": "String",
        "Optional": true,
        "Default": {
          "Type": "string",
          "Value": "custom"
        }
      }
    },
    "digitalocean_database_cluster": {
      "database": {
        "Type": "String",
        "Computed": true
      },
      "engine": {
        "Type": "String",
        "Required": true
      },
      "host": {
        "Type": "String",
        "Computed": true
      },
      "maintenance_window": {
        "Type": "List",
        "Optional": true,
        "MinItems": 1,
        "Elem": {
          "Type": "SchemaInfo",
          "Info": {
            "day": {
              "Type": "String",
              "Required": true
            },
            "hour": {
              "Type": "String",
              "Required": true
            }
          }
        }
      },
      "name": {
        "Type": "String",
        "Required": true
      },
      "node_count": {
        "Type": "Int",
        "Required": true
      },
      "password": {
        "Type": "String",
        "Computed": true
      },
      "port": {
        "Type": "Int",
        "Computed": true
      },
      "region": {
        "Type": "String",
        "Required": true
      },
      "size": {
        "Type": "String",
        "Required": true
      },
      "uri": {
        "Type": "String",
        "Computed": true
      },
      "user": {
        "Type": "String",
        "Computed": true
      },
      "version": {
        "Type": "String",
        "Required": true
      }
    },
    "digitalocean_domain": {
      "ip_address": {
        "Type": "String",
        "Optional": true
      },
      "name": {
        "Type": "String",
        "Required": true
      },
      "urn": {
        "Type": "String",
        "Computed": true
      }
    },
    "digitalocean_droplet": {
      "backups": {
        "Type": "Bool",
        "Optional": true,
        "Default": {
          "Type": "bool",
          "Value": "false"
        }
      },
      "disk": {
        "Type": "Int",
        "Computed": true
      },
      "image": {
        "Type": "String",
        "Required": true
      },
      "ipv4_address": {
        "Type": "String",
        "Computed": true
      },
      "ipv4_address_private": {
        "Type": "String",
        "Computed": true
      },
      "ipv6": {
        "Type": "Bool",
        "Optional": true,
        "Default": {
          "Type": "bool",
          "Value": "false"
        }
      },
      "ipv6_address": {
        "Type": "String",
        "Computed": true
      },
      "ipv6_address_private": {
        "Type": "String",
        "Computed": true
      },
      "locked": {
        "Type": "Bool",
        "Computed": true
      },
      "memory": {
        "Type": "Int",
        "Computed": true
      },
      "monitoring": {
        "Type": "Bool",
        "Optional": true,
        "Default": {
          "Type": "bool",
          "Value": "false"
        }
      },
      "name": {
        "Type": "String",
        "Required": true
      },
      "price_hourly": {
        "Type": "Float",
        "Computed": true
      },
      "price_monthly": {
        "Type": "Float",
        "Computed": true
      },
      "private_networking": {
        "Type": "Bool",
        "Optional": true,
        "Default": {
          "Type": "bool",
          "Value": "false"
        }
      },
      "region": {
        "Type": "String",
        "Required": true
      },
      "resize_disk": {
        "Type": "Bool",
        "Optional": true,
        "Default": {
          "Type": "bool",
          "Value": "true"
        }
      },
      "size": {
        "Type": "String",
        "Required": true
      },
      "ssh_keys": {
        "Type": "Set",
        "Optional": true,
        "Elem": {
          "Type": "SchemaElements",
          "ElementsType": "String"
        }
      },
      "status": {
        "Type": "String",
        "Computed": true
      },
      "tags": {
        "Type": "Set",
        "Optional": true,
        "Elem": {
          "Type": "SchemaElements",
          "ElementsType": "String"
        }
      },
      "urn": {
        "Type": "String",
        "Computed": true
      },
      "user_data": {
        "Type": "String",
        "Optional": true
      },
      "vcpus": {
        "Type": "Int",
        "Computed": true
      },
      "volume_ids": {
        "Type": "Set",
        "Optional": true,
        "Computed": true,
        "Elem": {
          "Type": "SchemaElements",
          "ElementsType": "String"
        }
      }
    },
    "digitalocean_droplet_snapshot": {
      "created_at": {
        "Type": "String",
        "Computed": true
      },
      "droplet_id": {
        "Type": "String",
        "Required": true
      },
      "min_disk_size": {
        "Type": "Int",
        "Computed": true
      },
      "name": {
        "Type": "String",
        "Required": true
      },
      "regions": {
        "Type": "Set",
        "Computed": true,
        "Elem": {
          "Type": "SchemaElements",
          "ElementsType": "String"
        }
      },
      "size": {
        "Type": "Float",
        "Computed": true
      }
    },
    "digitalocean_firewall": {
      "created_at": {
        "Type": "String",
        "Computed": true
      },
      "droplet_ids": {
        "Type": "Set",
        "Optional": true,
        "Elem": {
          "Type": "SchemaElements",
          "ElementsType": "Int"
        }
      },
      "inbound_rule": {
        "Type": "Set",
        "Optional": true,
        "Elem": {
          "Type": "SchemaInfo",
          "Info": {
            "port_range": {
              "Type": "String",
              "Optional": true
            },
            "protocol": {
              "Type": "String",
              "Required": true
            },
            "source_addresses": {
              "Type": "Set",
              "Optional": true,
              "Elem": {
                "Type": "SchemaElements",
                "ElementsType": "String"
              }
            },
            "source_droplet_ids": {
              "Type": "Set",
              "Optional": true,
              "Elem": {
                "Type": "SchemaElements",
                "ElementsType": "Int"
              }
            },
            "source_load_balancer_uids": {
              "Type": "Set",
              "Optional": true,
              "Elem": {
                "Type": "SchemaElements",
                "ElementsType": "String"
              }
            },
            "source_tags": {
              "Type": "Set",
              "Optional": true,
              "Elem": {
                "Type": "SchemaElements",
                "ElementsType": "String"
              }
            }
          }
        }
      },
      "name": {
        "Type": "String",
        "Required": true
      },
      "outbound_rule": {
        "Type": "Set",
        "Optional": true,
        "Elem": {
          "Type": "SchemaInfo",
          "Info": {
            "destination_addresses": {
              "Type": "Set",
              "Optional": true,
              "Elem": {
                "Type": "SchemaElements",
                "ElementsType": "String"
              }
            },
            "destination_droplet_ids": {
              "Type": "Set",
              "Optional": true,
              "Elem": {
                "Type": "SchemaElements",
                "ElementsType": "Int"
              }
            },
            "destination_load_balancer_uids": {
              "Type": "Set",
              "Optional": true,
              "Elem": {
                "Type": "SchemaElements",
                "ElementsType": "String"
              }
            },
            "destination_tags": {
              "Type": "Set",
              "Optional": true,
              "Elem": {
                "Type": "SchemaElements",
                "ElementsType": "String"
              }
            },
            "port_range": {
              "Type": "String",
              "Optional": true
            },
            "protocol": {
              "Type": "String",
              "Required": true
            }
          }
        }
      },
      "pending_changes": {
        "Type": "List",
        "Computed": true,
        "Elem": {
          "Type": "SchemaInfo",
          "Info": {
            "droplet_id": {
              "Type": "Int",
              "Optional": true
            },
            "removing": {
              "Type": "Bool",
              "Optional": true
            },
            "status": {
              "Type": "String",
              "Optional": true
            }
          }
        }
      },
      "status": {
        "Type": "String",
        "Computed": true
      },
      "tags": {
        "Type": "Set",
        "Optional": true,
        "Elem": {
          "Type": "SchemaElements",
          "ElementsType": "String"
        }
      }
    },
    "digitalocean_floating_ip": {
      "droplet_id": {
        "Type": "Int",
        "Optional": true
      },
      "ip_address": {
        "Type": "String",
        "Optional": true,
        "Computed": true
      },
      "region": {
        "Type": "String",
        "Required": true
      },
      "urn": {
        "Type": "String",
        "Description": "the uniform resource name for the floating ip",
        "Computed": true
      }
    },
    "digitalocean_floating_ip_assignment": {
      "droplet_id": {
        "Type": "Int",
        "Required": true
      },
      "ip_address": {
        "Type": "String",
        "Required": true
      }
    },
    "digitalocean_kubernetes_cluster": {
      "cluster_subnet": {
        "Type": "String",
        "Computed": true
      },
      "created_at": {
        "Type": "String",
        "Computed": true
      },
      "endpoint": {
        "Type": "String",
        "Computed": true
      },
      "ipv4_address": {
        "Type": "String",
        "Computed": true
      },
      "kube_config": {
        "Type": "Set",
        "Computed": true,
        "Elem": {
          "Type": "SchemaInfo",
          "Info": {
            "client_certificate": {
              "Type": "String",
              "Computed": true
            },
            "client_key": {
              "Type": "String",
              "Computed": true
            },
            "cluster_ca_certificate": {
              "Type": "String",
              "Computed": true
            },
            "host": {
              "Type": "String",
              "Computed": true
            },
            "raw_config": {
              "Type": "String",
              "Computed": true
            }
          }
        }
      },
      "name": {
        "Type": "String",
        "Required": true
      },
      "node_pool": {
        "Type": "List",
        "Required": true,
        "MaxItems": 1,
        "MinItems": 1,
        "Elem": {
          "Type": "SchemaInfo",
          "Info": {
            "id": {
              "Type": "String",
              "Computed": true
            },
            "name": {
              "Type": "String",
              "Required": true
            },
            "node_count": {
              "Type": "Int",
              "Required": true
            },
            "nodes": {
              "Type": "List",
              "Computed": true,
              "Elem": {
                "Type": "SchemaInfo",
                "Info": {
                  "created_at": {
                    "Type": "String",
                    "Computed": true
                  },
                  "id": {
                    "Type": "String",
                    "Computed": true
                  },
                  "name": {
                    "Type": "String",
                    "Computed": true
                  },
                  "status": {
                    "Type": "String",
                    "Computed": true
                  },
                  "updated_at": {
                    "Type": "String",
                    "Computed": true
                  }
                }
              }
            },
            "size": {
              "Type": "String",
              "Required": true
            },
            "tags": {
              "Type": "Set",
              "Optional": true,
              "Elem": {
                "Type": "SchemaElements",
                "ElementsType": "String"
              }
            }
          }
        }
      },
      "region": {
        "Type": "String",
        "Required": true
      },
      "service_subnet": {
        "Type": "String",
        "Computed": true
      },
      "status": {
        "Type": "String",
        "Computed": true
      },
      "tags": {
        "Type": "Set",
        "Optional": true,
        "Elem": {
          "Type": "SchemaElements",
          "ElementsType": "String"
        }
      },
      "updated_at": {
        "Type": "String",
        "Computed": true
      },
      "version": {
        "Type": "String",
        "Required": true
      }
    },
    "digitalocean_kubernetes_node_pool": {
      "cluster_id": {
        "Type": "String",
        "Required": true
      },
      "name": {
        "Type": "String",
        "Required": true
      },
      "node_count": {
        "Type": "Int",
        "Required": true
      },
      "nodes": {
        "Type": "List",
        "Computed": true,
        "Elem": {
          "Type": "SchemaInfo",
          "Info": {
            "created_at": {
              "Type": "String",
              "Computed": true
            },
            "id": {
              "Type": "String",
              "Computed": true
            },
            "name": {
              "Type": "String",
              "Computed": true
            },
            "status": {
              "Type": "String",
