      "name": {
        "Type": "String",
        "Required": true
      },
      "port": {
        "Type": "Int",
        "Optional": true
      },
      "priority": {
        "Type": "Int",
        "Optional": true
      },
      "tag": {
        "Type": "String",
        "Optional": true
      },
      "ttl": {
        "Type": "Int",
        "Optional": true,
