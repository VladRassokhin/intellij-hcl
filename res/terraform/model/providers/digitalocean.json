            "cookie_name": {
              "Type": "String",
              "Optional": true
            },
            "cookie_ttl_seconds": {
              "Type": "Int",
              "Optional": true
            },
            "type": {
              "Type": "String",
              "Optional": true,
              "Default": {
                "Type": "string",
                "Value": "none"
              }
            }
          }
        }
      },
      "urn": {
        "Type": "String",
        "Description": "the uniform resource name for the load balancer",
        "Computed": true
      }
    },
    "digitalocean_project": {
      "created_at": {
        "Type": "String",
        "Description": "the date and time when the project was created, (ISO8601)",
        "Computed": true
      },
      "description": {
        "Type": "String",
        "Optional": true,
        "Description": "the descirption of the project",
        "Default": {
          "Type": "string"
        }
      },
      "environment": {
        "Type": "String",
        "Optional": true,
        "Description": "the environment of the project's resources",
