{
  "name": "s3",
  "type": "backend",
  "schema": {
    "bucket": {
      "Type": "TypeString",
      "Optional": true
    },
    "key": {
      "Type": "TypeString",
      "Optional": true
    },
    "region": {
      "Type": "TypeString",
      "Optional": true,
      "Default": {
        "type": "string",
        "value": "AWS_DEFAULT_REGION"
      }
    },
    "endpoint": {
      "Type": "TypeString",
      "Optional": true
    },
    "encrypt": {
      "Type": "TypeBool",
      "Optional": true
    },
    "acl": {
      "Type": "TypeString",
      "Optional": true
    },
    "access_key": {
      "Type": "TypeString",
      "Optional": true
    },
    "secret_key": {
      "Type": "TypeString",
      "Optional": true
    },
    "kms_key_id": {
      "Type": "TypeString",
      "Optional": true
    },
    "lock_table": {
      "Type": "TypeString",
      "Optional": true
    },
    "dynamodb_table": {
      "Type": "TypeString",
      "Optional": true
    },
    "profile": {
      "Type": "TypeString",
      "Optional": true
    },
    "shared_credentials_file": {
      "Type": "TypeString",
      "Optional": true
    },
    "token": {
      "Type": "TypeString",
      "Optional": true
    },
    "role_arn": {
      "Type": "TypeString",
      "Optional": true
    },
    "session_name": {
      "Type": "TypeString",
      "Optional": true
    },
    "external_id": {
      "Type": "TypeString",
      "Optional": true
    },
    "assume_role_policy": {
      "Type": "TypeString",
      "Optional": true
    },
    "workspace_key_prefix": {
      "Type": "TypeString",
      "Optional": true
    },
    "skip_credentials_validation": {
      "Type": "TypeBool",
      "Optional": true
    },
    "skip_get_ec2_platforms": {
      "Type": "TypeBool",
      "Optional": true
    },
    "skip_region_validation": {
      "Type": "TypeBool",
      "Optional": true
    },
    "skip_requesting_account_id": {
      "Type": "TypeBool",
      "Optional": true
    },
    "skip_metadata_api_check": {
      "Type": "TypeBool",
      "Optional": true
    },
    "force_path_style": {
      "Type": "TypeBool",
      "Optional": true,
      "Description": "Force s3 to use path style api.",
      "Default": {
        "Type": "bool",
        "Value": "false"
      }
    }
  }
}
